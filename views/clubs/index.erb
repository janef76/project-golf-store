<h2>All Clubs</h2>

<table>
  <tr>
    <th>Club Name</th>
    <th>Club Type</th>
    <th>Manufacturer Name</th>
    <th>Quantity</th>
    <th>Stock Indicator</th>
  </tr>

<% for club in @clubs %>
  <tr>
<td><%= club.name %></td>
<td><%= club.type %></td>
<td><%= Manufacturer.manufacturer_name(club.manufacturer_id) %></td>
<td><%= club.quantity %></td>
<td><%= club.level_indicator %></td>
  </tr>

<!-- <p> <%= club.name %> <%= club.type %> <%= Manufacturer.manufacturer_name(club.manufacturer_id) %> <%= club.quantity %> <%= club.level_indicator %> </p> -->
<% end %>
</table>
